{
  "backup_strategy": {
    "data_backup": {
      "frequency": "hourly",
      "retention": "30_days"
    },
    "config_backup": {
      "frequency": "daily",
      "retention": "90_days"
    },
    "code_backup": {
      "frequency": "on_change",
      "retention": "indefinite"
    }
  },
  "recovery_procedures": [
    "assess_damage",
    "restore_from_backup",
    "verify_system_integrity",
    "resume_operations"
  ],
  "rto_targets": {
    "critical_services": 60,
    "normal_services": 300,
    "non_critical_services": 1800
  }
}